dofile("script/include/inc_all.lua")
dofile("script/include/inc_event.lua")
function groundInit()
end
function groundStart()
  subMapCameraDefMode()
  GM("TOMODACHI_POST_TEGAMI"):SetVisible(false)
  GM("TOMODACHI_POST"):SetVisible(true)
  CAMERA:SetEye(SymCam("CAMERA_00"))
  CAMERA:SetTgt(SymCam("CAMERA_00"))
  CAMERA:MoveFollow2(SymCam("CAMERA_01"), Speed(1.5, ACCEL_TYPE.NONE, DECEL_TYPE.HIGH))
  SOUND:PlayBgm(SymSnd("BGM_EVE_HAPPY"), Volume(256))
  SCREEN_A:FadeIn(TimeSec(0.5), true)
  CAMERA:WaitMove()
  SCREEN_A:FadeOut(TimeSec(0.3), true)
  CAMERA:SetEye(SymCam("CAMERA_02"))
  CAMERA:SetTgt(SymCam("CAMERA_02"))
  TASK:Sleep(TimeSec(0.3))
  SCREEN_A:FadeIn(TimeSec(0.3), true)
  subEveFadeAfterTime()
  SOUND:PlaySe(SymSnd("SE_EVT_SIGN_NOTICE_LOW_01"), Volume(256))
  CH("NUOO"):SetManpu("MP_EXCLAMATION")
  CH("NUOO"):WaitManpu()
  WINDOW:DrawFace(20, 20, SymAct("NUOO"), FACE_TYPE.EMOTION)
  WINDOW:Talk(SymAct("NUOO"), -1328316672)
  WINDOW:DrawFace(20, 20, SymAct("NUOO"), FACE_TYPE.NORMAL)
  WINDOW:Talk(SymAct("NUOO"), -1446490559)
  WINDOW:Talk(SymAct("NUOO"), -2098913918)
  WINDOW:Talk(SymAct("NUOO"), -1677841213)
  WINDOW:Talk(SymAct("NUOO"), -725632508)
  WINDOW:Talk(SymAct("NUOO"), -844854459)
  WINDOW:Talk(SymAct("NUOO"), -427173754)
  WINDOW:Talk(SymAct("NUOO"), -7149113)
  WINDOW:Talk(SymAct("NUOO"), 2013984008)
  WINDOW:Talk(SymAct("NUOO"), 1628554313)
  WINDOW:Talk(SymAct("NUOO"), -426139881)
  WINDOW:Talk(SymAct("NUOO"), -8212906)
  WINDOW:Talk(SymAct("NUOO"), -726663787)
  WINDOW:CloseMessage()
  CAMERA:SetEye(SymCam("CAMERA_03"))
  CAMERA:SetTgt(SymCam("CAMERA_03"))
  TASK:Sleep(TimeSec(0.3))
  CH("NUOO"):SetManpu("MP_SPREE_LP")
  CH("NUOO"):SetMotion(SymMot("LETSGO"), LOOP.OFF)
  WINDOW:DrawFace(72, 16, SymAct("NUOO"), FACE_TYPE.GLADNESS)
  WINDOW:Talk(SymAct("NUOO"), -843789100)
  CH("NUOO"):WaitPlayMotion()
  WINDOW:CloseMessage()
  SOUND:FadeOutBgm(TimeSec(1.5))
  SCREEN_A:FadeOut(TimeSec(0.5), true)
  SOUND:WaitBgm()
  CH("NUOO"):ResetManpu()
  FLAG.MapFlags = CONST.MAP_CROSSROAD
  FLAG.Para1stIn = CONST.FLAG_FALSE
  SYSTEM:NextMapEntry(CONST.MAP_PARA_CENTER)
end
function groundEnd()
end

